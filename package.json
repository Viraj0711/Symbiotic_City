{
  "name": "symbiotic-city",
  "version": "1.0.0",
  "description": "Symbiotic City - Community Sustainability Platform",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "cd backend && npm install && cd ../frontend && npm install",
    "start": "npm run dev",
    "clean": "cd backend && npm run clean && cd ../frontend && npm run clean"
  },
  "keywords": [
    "sustainability",
    "community",
    "environment",
    "marketplace",
    "react",
    "nodejs",
    "typescript"
  ],
  "author": "Symbiotic City Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/symbiotic-city.git"
  }
}
